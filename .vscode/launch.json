{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GoLang: Current File",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Test Client",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmark/ujrpc_client.py",
            "console": "integratedTerminal",
            "args": [
                "-d"
            ],
            "justMyCode": false,
            "subProcess": true
        },
        {
            "name": "C++: Test io_uring Server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_debug/ujrpc_server_uring_bench",
            "args": [
                "--gtest_break_on_failure",
                "--gtest_catch_exceptions=0"
            ],
            "cwd": "${workspaceFolder}",
            "environment": [],
            "showDisplayString": true,
            "stopAtEntry": false,
            "externalConsole": false,
            "preLaunchTask": "Build Debug",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "C++: Test POSIX Server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_debug/ujrpc_server_posix_bench",
            "args": [
                "--gtest_break_on_failure",
                "--gtest_catch_exceptions=0"
            ],
            "cwd": "${workspaceFolder}",
            "environment": [],
            "showDisplayString": true,
            "stopAtEntry": false,
            "externalConsole": false,
            "preLaunchTask": "Build Debug",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb"
        },
    ]
}